@page "/"

<CascadingAuthenticationState>
    <AuthorizeView>
        <Authorized>
            <h1>Welcome, @context?.User?.Identity?.Name!</h1>
        </Authorized>
        <NotAuthorized>
            <h1>Bạn không có quyền truy cập~~</h1>
        </NotAuthorized>
    </AuthorizeView>
</CascadingAuthenticationState>


